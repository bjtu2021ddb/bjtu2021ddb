[is="dist/goods-action-button/index"] {
    flex: 1;
}

[is="dist/icon/index"] {
    display: inline-flex;
    align-items: center;
    justify-content: center;
}

[is="dist/loading/index"] {
    font-size: 0;
    line-height: 1;
}

[is="dist/tab/index"] {
    flex-shrink: 0;
    width: 100%;
}

[is="dist/tabbar-item/index"] {
    flex: 1;
}

.page {
    width: 100%;
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
    font-weight: 400;
    font-family: PingFangSC-regular;
    color: #353535;
}

.line,.page,page {
    background-color: #f2f2f2;
}

.line {
    width: 90%;
    margin-left: 5%;
    height: 2rpx;
}

.anotheline {
    width: 94%;
    margin-left: 3%;
}

.allLine,.anotheline {
    height: 2rpx;
    background-color: #f0f0f2;
}

.allLine {
    width: 100%;
}

.aliNocenter,.spaceBetween {
    display: flex;
    align-items: center;
}

.spaceBetween {
    justify-content: space-between;
}

.alignAround,.spaceAround {
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.alignAround {
    flex-direction: column;
}

.btnback {
    background-image: linear-gradient(90deg,#e94e3a,#f85d1b);
    border-radius: 20rpx;
}

.flexwarp {
    display: flex;
    flex-wrap: wrap;
}

.allCenter {
    justify-content: center;
}

.alicenter,.allCenter {
    flex-direction: column;
}

.alicenter,.allCenter,.allJustCenter {
    display: flex;
    align-items: center;
}

.allJustCenter {
    justify-content: center;
}

.textOverflow {
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.textOverflow,.textoverflowline {
    text-overflow: ellipsis;
    overflow: hidden;
}

.textoverflowline {
    white-space: nowrap;
}

.seracrContainer {
    width: 100%;
    height: 90rpx;
    background-color: #e32c1a;
}

.seracrContainer .input {
    border-radius: 8rpx;
    font-size: 25rpx;
    width: 90%;
    margin-left: 5%;
    height: 60rpx;
    background-color: hsla(0,0%,100%,.5);
    color: hsla(0,0%,100%,.5);
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 15rpx;
}

.seracrContainer .input input {
    width: 90%;
    margin-left: 5%;
    color: hsla(0,0%,100%,.5);
    padding-left: 5rpx;
}

.backgrounColor {
    background-color: #fff;
    border-radius: 20rpx;
    box-shadow: 0 2px 7px 0 hsla(0,0%,81.2%,.5);
}

.marginLeft {
    width: 94%;
    margin-left: 3%;
}

.marginLeftMInwidth {
    width: 96%;
    margin-left: 2%;
}

.marginLeftMaxwidth {
    width: 90%;
    margin-left: 5%;
}

.notokenbutton {
    width: 60%;
    height: 75rpx;
    background-image: linear-gradient(90deg,#e94e3a,#f85d1b);
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 200rpx;
    color: #fff;
}

.nothingBannewr {
    width: 100%;
    height: 400rpx;
}