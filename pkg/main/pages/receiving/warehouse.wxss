.title {
    height: 80rpx;
    font-size: 28rpx;
    color: #e64340;
}

.warehouseContainer {
    width: 90%;
    margin-left: 5%;
    border-radius: 20rpx;
    padding-bottom: 20rpx;
}

.bigweig,.nocontry {
    font-size: 34rpx;
    margin: 10rpx 0;
}

.nocontry {
    display: flex;
    justify-content: end;
    align-items: center;
    color: #e64340;
}

.copycontainer {
    width: 60%;
    border-radius: 8rpx;
    padding: 8rpx 20rpx 32rpx 8rpx;
    border: 1rpx solid #c1c1c1;
    font-size: 28rpx;
    flex-wrap: wrap;
    position: relative;
}

.colorFont {
    color: #409eff;
}

.margin {
    position: absolute;
    right: 8rpx;
    bottom: 4rpx;
    margin-left: 20rpx;
}

.fontweight {
    font-size: 28rpx;
}

.marginbottom {
    margin-bottom: 15rpx;
}

.tip {
    margin-top: 40rpx;
}

.tip,.tips {
    font-size: 24rpx;
    color: #e64340;
}

.tips {
    padding: 20rpx 0;
    font-weight: 500;
}

.addrestip {
    width: 100%;
    text-align: center;
    background-image: linear-gradient(90deg,#e94e3a,#f85d1b);
    color: #fff;
    font-size: 28rpx;
    margin-bottom: 30rpx;
    height: 100rpx;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.addrestip text {
    margin: 0 20rpx;
}